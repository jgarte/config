Rxvt.scrollBar: false
Rxvt.lineSpace: 0
Rxvt.letterSpace: 0
Rxvt.cursorColor: goldenrod
Rxvt.shading: 25
Rxvt.background: #222222
Rxvt.foreground: white
Rxvt.tintColor: gray70

Xft*dpi: 82
Xft*antialias:  true
Xft*hinting:    true
! Xft*rgba:       rgb
Xft*rgba:       none
Xft*autohint:   false
Xft*hintstyle:  hintfull
Xft*lcdfilter:  lcddefault

URxvt.font: 9x15

urxvt.font: xft:SFMonoSquare Nerd Font:style=Regular:size=16:antialias=true
urxvt.boldFont: xft:SFMonoSquare Nerd Font:style=Bold:size=16:antialias=true

! urxvt.font: xft:SF Mono Square:style=Regular:size=12:antialias=true
! urxvt.boldFont: xft:SF Mono Square:style=Bold:size=12:antialias=true

! urxvt.font: xft:HackGenConsole Nerd Font:size=9:style=normal:antialias=true
! urxvt.boldFont: xft:HackGenConsole Nerd Font:size=9:style=bold:antialias=true

! urxvt.font: xft:HackGen Console for Powerline:size=9:style=normal:antialias=true
! urxvt.boldFont: xft:HackGen Console for Powerline:size=9:style=bold:antialias=true

! urxvt.font: xft:RictyDiscord Nerd Font:size=9:style=normal:antialias=true
! urxvt.boldFont: xft:RictyDiscord Nerd Font:size=9:style=bold:antialias=true

! urxvt.font: xft:Ricty Discord:size=9:style=normal
! urxvt.boldFont: xft:Ricty Discord:size=9:style=bold

! urxvt.font: xft:Ricty Diminished with\-icons:size=12:style=Regular
! urxvt.boldFont: xft:Ricty Diminished with\-icons:size=12:style=Bold
urxvt.transparent: false
