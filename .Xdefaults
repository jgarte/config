urxvt*font:\
        x:-mplus-fxd-medium-r-normal--10-*-*-*-*-iso8859-*,\
        x:-mplus-gothic-medium-r-normal--10-*-*-*-*-jisx0208.1990-*
urxvt*bold:\
        x:-mplus-fxd-medium-r-normal--10-*-*-*-*-iso8859-*,\
        x:-mplus-gothic-medium-r-normal--10-*-*-*-*-jisx0208.1990-*
Rxvt.lineSpace: 0
Rxvt.background: gray10
Rxvt.foreground: gray80
Rxvt.tintColor: gray70
Rxvt.cursorColor: goldenrod
! Rxvt.fading: 50
urxvt.transparent: true
urxvt.fading: 70
urxvt.fadecolor: [0]black
Rxvt.shading: 30
Rxvt.scrollBar: false
urxvt*color0: #000000
urxvt*color1: #CD0000
urxvt*color2: #00CD00
urxvt*color3: #CDCD00
urxvt*color4: #1E90FF
urxvt*color5: #CD00CD
urxvt*color6: #00CDCD
urxvt*color7: #E5E5E5
urxvt*color8: #4C4C4C
urxvt*color9: #FF0000
urxvt*color10: #00FF00
urxvt*color11: #FFFF00
urxvt*color12: #4672B4
urxvt*color13: #FF00FF
urxvt*color14: #00FFFF
urxvt*color15: #FFFFFF

! urxvt.font: [codeset=ISO8859]x:-mplus-fxd-medium-r-normal-*-10-*-*-*-*-*-iso8859-*, \
!             [codeset=JISX0208]x:-mplus-gothic-medium-r-normal--10-*-*-*-*-*-jisx0208.1990-*, 5x10
! urxvt.boldFont: [codeset=ISO8859]x:-mplus-fxd-medium-r-normal-*-10-*-*-*-*-*-iso8859-*, \
!                 [codeset=JISX0208]x:-mplus-gothic-bold-r-normal--10-*-*-*-*-*-jisx0208.1990-*, 5x10
